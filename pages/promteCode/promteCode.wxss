page{
	background: #14cebc;
}
.container{
	width: 100%;
	height: 100vh;
	display: flex;
	justify-content: center;
	align-items: center;
}
.code-content{
	width: 90%;
	min-height: 90%;
	background: #FFFFFF;
	border-radius: 24rpx;
	display: flex;
	flex-direction: column;
}
.share-btn{
	position: absolute;
	right: 30rpx;
	top: 50%;
	transform: translateY(-50%);
	background-color: #15CEBC !important;
	color: #FFFFFF;
}
.code-top{
	width: 100%;
	box-sizing: border-box;
	padding: 36rpx;
	display: flex;
	position: relative;
}
.code-top image{
	display: block;
	width: 108rpx;
	height: 108rpx;
	border-radius: 50% 50%;
}
.code-t-tit{
	padding-left: 30rpx;
}
.code-t-tit text{
	padding-top: 10rpx;
	display: block;
}
.code-t-tit text.tit{
	font-size: 28rpx;
}
.code-t-tit text.name{
	font-size: 32rpx;
}
.code-conter{
	width: 100%;
	min-height: 60%;
	text-align: center;
}
.code-conter .img{
	display: inline-block;
	width: 200rpx;
	height: 160rpx;
	overflow: hidden;
	padding-top: 30rpx;
}
.code-conter .img image{
	display: block;
	width: 100%;
}
.code-c-txt text{
	display: block;
	font-size: 34rpx;
}
.code-c-li{
	padding-top: 20rpx;
}
.code-bottom{
	width: 100%;
	padding-top: 20rpx;
}
.code-bottom .code-img{
	display: block;
	width: 350rpx;
	
	margin: 0 auto;
}
.code-bottom .code-img image{
	width: 100%;
	height: 350rpx;
	background-image: url("data:image/svg+xml;base64,");
	background-size: 80% 80%;
	background-position: center;
	background-repeat: no-repeat;
}
.code-tip{
	width: 100%;
	height: 68rpx;
	position: relative;
	margin: 30rpx 0;
}
.code-tip text{
	position: absolute;
	width: 68rpx;
	height: 68rpx;
	background: #14cebc;
	top: 0;
	border-radius: 50% 50%;
	z-index: 2;
}
.code-tip:after{
	position: absolute;
	content: " ";
	width: 100%;
	top: 50%;
	left: 0;
	z-index: 1;
	border:1px dashed #eacc20;
}
.tip-left{
	left: -34rpx;
}
.tip-right{
	right: -34rpx;
}
.code-input{
	width: 460rpx;
	margin: 0 auto;
}
.code-form{
	display: flex;
}
.code-form{
	width: 100%;
	margin-bottom: 20rpx;
}
.phone{
	display: block;
	width: 100%;
	height: 90rpx;
	border: 1px solid #F2F2F2;
	box-sizing: border-box;
	background-color: #d0f5f2;
	border-radius: 12rpx;
	padding-left: 65rpx;
	background-image: url(data:image/png;base64,);
	background-position: 20rpx center;
	background-repeat: no-repeat no-repeat;
	background-size: 26rpx auto;
}
.vcode{
	display: block;
	flex: 1;
	height: 90rpx;
	border: 1px solid #F2F2F2;
	box-sizing: border-box;
	background-color: #d0f5f2;
	border-radius: 12rpx;
	padding-left: 65rpx;
	background-image: url(data:image/png;base64,);
	background-position: 15rpx center;
	background-repeat-x: no-repeat;
	background-repeat-y: no-repeat;
	background-size: 36rpx auto;
}
.vcode-btn{
	display: block;
	height: 90rpx;
	border: 1px solid #F2F2F2;
	line-height: 90rpx;
	padding: 0 20rpx;
	border-radius: 12rpx;
	margin-left: 10rpx;
}
.form-btn{
	padding-top: 20rpx;
}
.form-btn button{
	height: 90rpx;
	line-height: 90rpx;
}
.code-input .ok,.code-input .ph{
	display: block;
	text-align: center;
	font-size: 32rpx;
	padding-bottom: 20rpx;
}
.code-input.al{
	text-align: center;
	padding-top: 30rpx;
}
.al-text{
	text-align: center;
	font-size: 48rpx;
	background: linear-gradient(to bottom, #f9f5b6, #53a0bc);
    -webkit-background-clip: text;
    color: transparent;
}
.al-btn{
	display: inline-block;
	padding: 15rpx 40rpx;
	border-radius: 40rpx;
	background: #03d0bc;
	font-size: 30rpx;
	color: #ffffff;
	margin-top: 40rpx;
	box-shadow: 0px 10rpx 10rpx rgba(3, 208, 188, 0.22);
}